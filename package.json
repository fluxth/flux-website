    "autoprefixer": "10.4.22",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-svelte": "3.12.5",
    "globals": "16.4.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.43.0",
    "svelte-check": "4.3.3",
    "tailwindcss": "3.4.18",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.12",
    "vitest": "4.0.4"
