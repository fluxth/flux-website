    "autoprefixer": "10.4.20",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-svelte": "2.46.1",
    "globals": "15.14.0",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.3.3",
    "svelte": "5.19.1",
    "svelte-check": "4.1.4",
    "tailwindcss": "3.4.17",
    "tslib": "2.8.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.21.0",
    "vite": "6.0.11",
    "vitest": "2.1.6"
