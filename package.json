    "autoprefixer": "10.4.19",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.40.0",
    "globals": "15.6.0",
    "postcss": "8.4.38",
    "prettier": "3.3.2",
    "prettier-plugin-svelte": "3.2.4",
    "svelte": "4.2.18",
    "svelte-check": "3.8.1",
    "tailwindcss": "3.4.4",
    "tslib": "2.6.3",
    "typescript": "5.4.5",
    "typescript-eslint": "8.0.0-alpha.30",
    "vite": "5.3.1",
    "vitest": "1.6.0"
