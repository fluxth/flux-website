    "autoprefixer": "10.4.21",
    "eslint": "9.30.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-svelte": "3.9.2",
    "globals": "16.2.0",
    "postcss": "8.5.6",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.34.5",
    "svelte-check": "4.2.2",
    "tailwindcss": "3.4.17",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1",
    "vite": "6.3.5",
    "vitest": "3.2.4"
