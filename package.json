    "autoprefixer": "10.4.21",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-svelte": "3.6.0",
    "globals": "16.1.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.28.6",
    "svelte-check": "4.1.7",
    "tailwindcss": "3.4.17",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1",
    "vite": "6.3.5",
    "vitest": "3.1.3"
